#!/bin/bash

# functions only

# $1 is the ${REPOS}
init_repo()
{
	# check the repo dir again
	if [[ ! -d $1 ]]; then
		mkdir -p $1/conf
		# create repo's base
		# default to mips64el
		echo "Origin: Deepin
Label: Deepin
Suite: unstable stable trusty
Codename: unstable 
Architectures: mips64el source 
Components: main non-free contrib
UDebComponents: main non-free contrib
Version: 1.0
Description: Deepin mips64el
Update: shuttle" > $1/conf/distributions

		# create the default repo dirs
		reprepro -b $1 export

		# enable this repo for build env, link it to http server
		sudo mkdir -p /var/www/html/repos/
		sudo ln -sf $1 /var/www/html/repos/mips64el
	fi
}

# $1 is the X86_LIST
create_x86_list()
{
	echo "deb-src [trusted=yes] http://pools.corp.deepin.com/deepin unstable main contrib non-free" > $1
}

